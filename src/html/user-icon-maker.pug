doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(content="initial-scale=1, shrink-to-fit=no, width=device-width" name="viewport")
    link(href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i|Roboto+Mono:300,400,700|Roboto+Slab:300,400,700" rel="stylesheet")
    link(href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
    link(href="css/material-custom.min.css" rel="stylesheet")
    title
      |User Icon Maker - Questetra Support Tools
  body
    header.navbar.navbar-dark.navbar-full.bg-primary.doc-navbar-default.fixed-top
      button.navbar-toggler(aria-controls="navdrawerLeft" aria-expanded="false" aria-label="Toggle Navdrawer" data-target="#navdrawerLeft" data-toggle="navdrawer")
        span.navbar-toggler-icon
      a.navbar-brand.mr-auto(href="index.html")
        |Questetra Support Tools
    div#navdrawerLeft.navdrawer(aria-hidden="true" tabindex="-1")
      div.navdrawer-content
        div.navdrawer-header
          a.navbar-brand.px-0(href="index.html")
            |Questetra Support Tools
        nav.navdrawer-nav
          a.nav-item.nav-link.active(href="ui-decorator.html")
            i.material-icons.mr-2
              |style
            |User Icon Maker
          //div.navdrawer-divider
          //p.navdrawer-subheader
          //  |Navdrawer subheader
    div.drawer-container-layout
      div.container-fluid
        div.row
          div.col
            nav(aria-label="breadcrumb")
             ol.breadcrumb
               li.breadcrumb-item(aria-current="page")
                 a(href="index.html")
                   |Home
               li.breadcrumb-item.active(aria-current="page")
                 |User Icon Maker
            h2
              i.material-icons.mr-2
                |style
              |User Icon Maker
        div.row
          div.col
            p(style="margin:40px 0 40px 0;")
              |***
        div.row
          div.col
            div#GuidePanelMaker(style="margin:40px 0 80px 0;")

    script(src="https://code.jquery.com/jquery-3.3.1.slim.min.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js")
    script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js")
    script(src="js/material.min.js")
    script(src="js/responsiveDrawer.js")

